ror  });\n\n          }else{\n          this.props.login(user)\n          localStorage.setItem(\"token\", user.jwt)\n          }\n          this.props.history.push('/rooms')\n        })\n        this.setState({\n          username: \"\",\n          password: \"\",\n        })\n      }\n\n    handleChange(e) {\n        this.setState({\n          [e.target.name]: e.target.value,\n          [e.target.name]: e.target.value\n        });\n      }\n\n    render() {\n        return(\n          \n          \n          <div id=\"loginApp\">\n            {this.state.error ? <h1 style={{color: \"red\"}}> {this.state.error} </h1> : null}\n            <div >\n\n            <Grid id=\"loginApp\"  textAlign='center' style={{ height: '100vh' }} verticalAlign='middle'>\n              <Grid.Column style={{ maxWidth: 450 }}>\n                <img className=\"logo\" style={{ maxWidth: 450 }} src={Logo} alt={null} /> \n                <img className=\"header\"style={{ maxWidth: 450 }} src={MainHeader}  alt={null} />        \n                <Form size='large' onSubmit={this.handleSubmit}>\n                  <Segment stacked>\n                    <Form.Field \n                      fluid \n                      icon='user' \n                      iconPosition='left' \n                      placeholder='E-mail address' >\n                      <input type=\"text\" name=\"username\" placeholder=\"SIGN IN\" onChange={(event) => this.handleChange(event)} value={this.state.username}/>\n                    </Form.Field> \n                    \n                    <Form.Field\n                      fluid icon='lock'\n                      iconPosition='left'\n                      placeholder='Password'\n                      type='password'\n                    >\n                      <input type=\"password\" name=\"password\" placeholder=\"PASSWORD\" onChange={(event) => this.handleChange(event)} value={this.state.password}/>  \n                    </Form.Field>\n                    <Button color='teal' fluid size='large'>\n                      Login\n                    </Button>\n                  </Segment>\n                </Form>\n              </Grid.Column>\n            </Grid>\n            </div>\n          </div>     \n       );\n    }\n}\n\n\nexport default connect(null, { login })(Login);\n\n\n",{"ruleId":"97","severity":1,"message":"98","line":4,"column":33,"nodeType":"99","messageId":"100","endLine":4,"endColumn":43},{"ruleId":"97","severity":1,"message":"101","line":5,"column":9,"nodeType":"99","messageId":"100","endLine":5,"endColumn":15},{"ruleId":"97","severity":1,"message":"102","line":5,"column":16,"nodeType":"99","messageId":"100","endLine":5,"endColumn":22},{"ruleId":"97","severity":1,"message":"103","line":5,"column":24,"nodeType":"99","messageId":"100","endLine":5,"endColumn":33},{"ruleId":"97","severity":1,"message":"104","line":5,"column":35,"nodeType":"99","messageId":"100","endLine":5,"endColumn":39},{"ruleId":"97","severity":1,"message":"105","line":5,"column":41,"nodeType":"99","messageId":"100","endLine":5,"endColumn":47},{"ruleId":"97","severity":1,"message":"106","line":5,"column":55,"nodeType":"99","messageId":"100","endLine":5,"endColumn":62},{"ruleId":"97","severity":1,"message":"107","line":7,"column":7,"nodeType":"99","messageId":"100","endLine":7,"endColumn":26},{"ruleId":"97","severity":1,"message":"108","line":5,"column":10,"nodeType":"99","messageId":"100","endLine":5,"endColumn":18},{"ruleId":"97","severity":1,"message":"109","line":5,"column":20,"nodeType":"99","messageId":"100","endLine":5,"endColumn":31},{"ruleId":"97","severity":1,"message":"110","line":6,"column":24,"nodeType":"99","messageId":"100","endLine":6,"endColumn":28},{"ruleId":"111","severity":1,"message":"112","line":54,"column":11,"nodeType":"113","endLine":54,"endColumn":43},{"ruleId":"97","severity":1,"message":"114","line":5,"column":24,"nodeType":"99","messageId":"100","endLine":5,"endColumn":32},{"ruleId":"97","severity":1,"message":"109",